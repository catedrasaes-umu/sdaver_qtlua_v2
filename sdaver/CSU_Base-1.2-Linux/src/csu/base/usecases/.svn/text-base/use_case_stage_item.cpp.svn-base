#include "use_case_stage_item.h"

namespace csu {
namespace base {
namespace usecases {


UseCaseStageItem::UseCaseStageItem(const std::string & name,int x,int y,int width,int height)
{
    name_=name;
    x_=x;
    y_=y;
    width_=width;
    height_=height;
}


const std::string & UseCaseStageItem::name()
{
    return name_;
}

void UseCaseStageItem::name(const std::string & v)
{
    name_=v;
}

int UseCaseStageItem::x()
{
    return x_;
}

void UseCaseStageItem::x(int v)
{
    x_=v;
}

int UseCaseStageItem::y()
{
    return y_;
}

void UseCaseStageItem::y(int v)
{
    y_=v;
}

int UseCaseStageItem::width()
{
    return width_;
}

void UseCaseStageItem::width(int v)
{
    width_=v;
}

int UseCaseStageItem::height()
{
    return height_;
}

void UseCaseStageItem::height(int v)
{
    height_=v;
}

}
}
}
